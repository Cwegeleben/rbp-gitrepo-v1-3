#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules/@remix-run/serve/dist/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules/@remix-run/serve/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules/@remix-run/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules/@remix-run/serve/dist/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules/@remix-run/serve/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules/@remix-run/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/@remix-run+serve@2.17.0/node_modules:/Users/cwegeleben/Projects/rbp-gitrepo-v1-3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-run/serve/dist/cli.js" "$@"
else
  exec node  "$basedir/../@remix-run/serve/dist/cli.js" "$@"
fi
